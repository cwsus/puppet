[Match]
MACAddress=<%= scope.lookupvar("macaddress") %>

[Link]
MACAddress=<%= scope.lookupvar("macaddress") %>
MTUBytes=1500

<% if scope.lookupvar("static") == true %>
[Network]
Description=CWS
DNS=192.168.20.6 192.168.20.7
Domains=caspersbox.com network.caspersbox.com
<% if scope.lookupvar("vlan") %>
VLAN=<%= scope.lookupvar("vlan") %>
<% end %>

[Address]
Address=<%= scope.lookupvar("ip_addr") %>

[Route]
Gateway=<%= scope.lookupvar("gateway") %>
<% else %>
[Network]
DHCP=ipv4
DNS=192.168.20.6 192.168.20.7
Domains=caspersbox.com network.caspersbox.com
<% end %>
