#
# eSolutions tomcat-config.conf
#

<IfModule proxy_module>
    ProxyPass                       /eSolutions/        ajp://<%= scope.lookupvar("tomcat_host") %>:<%= scope.lookupvar("tomcat_port") %>/eSolutions/
    ProxyPassReverse                /eSolutions/        ajp://<%= scope.lookupvar("tomcat_host") %>:<%= scope.lookupvar("tomcat_port") %>/eSolutions/
</IfModule>

